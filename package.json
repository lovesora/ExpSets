{
  "name": "exp-sets",
  "version": "1.0.0",
  "description": "graduation project",
  "main": "src/app/router.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --inline --content-base --history-api-fallback",
    "start:prod": "webpack -p && node server.js",
    "start-srv": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start-srv:dev": "webpack-dev-server --inline --content-base --history-api-fallback",
    "start-srv:prod": "npm run build && node server.bundle.js",
    "build:client": "webpack -p",
    "build:server": "webpack --config webpack.server.config.js",
    "build": "npm run build:client && npm run build:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lovesora/ExpSets.git"
  },
  "keywords": [
    "webpack2",
    "react"
  ],
  "author": "liuxin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lovesora/ExpSets/issues"
  },
  "homepage": "https://github.com/lovesora/ExpSets#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.1",
    "moment": "^2.18.1",
    "node-sass": "^4.5.2",
    "path": "^0.12.7",
    "react-router": "3.0.2",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
